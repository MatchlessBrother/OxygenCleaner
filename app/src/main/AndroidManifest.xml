<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.oxygen.cleaner">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.KILL_BACKGROUND_PROCESSES" />

    <application
        android:supportsRtl="true"
        android:allowBackup="true"
        android:maxAspectRatio="2.4"
        android:theme="@style/AppTheme"
        android:label="@string/app_name"
        android:icon="@mipmap/ic_launcher"
        android:resizeableActivity="true">

        <activity
            android:name="com.oxygen.cleaner.pags.OxygenAct_Splash"
            android:theme="@style/NoStateBarStyle">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.oxygen.cleaner.pags.OxygenAct_Sder"
            android:theme="@style/LightStatusTheme" />

        <activity
            android:name="com.oxygen.cleaner.pags.OxygenAct_Basdst"
            android:theme="@style/LightStatusTheme" />

        <activity
            android:name="com.oxygen.cleaner.pags.OxygenAct_asdler"
            android:theme="@style/LightStatusTheme" />

        <activity
            android:name="com.oxygen.cleaner.pags.OxygenAct_White"
            android:theme="@style/LightStatusTheme" />

        <activity
            android:name="com.oxygen.cleaner.pags.OxygenAct_Cldan"
            android:theme="@style/LightStatusTheme" />

        <activity
            android:name="com.oxygen.cleaner.pags.OxygenAoutAct"
            android:theme="@style/LightStatusTheme" />

        <activity
            android:name="com.oxygen.cleaner.pags.OxygenAct_BaseMain"
            android:theme="@style/LightStatusTheme" />

        <activity
            android:name="com.oxygen.cleaner.pags.OxygenAct_Prvacy"
            android:theme="@style/LightStatusTheme" />
    </application>

    <meta-data
        android:name="android.max_aspect"
        android:value="2.4" />

    <!--适配华为（huawei）刘海屏-->
    <meta-data
        android:name="android.notch_support"
        android:value="true"/>
    <!--适配小米（xiaomi）刘海屏-->
    <meta-data
        android:name="notch.config"
        android:value="portrait|landscape" />

</manifest>